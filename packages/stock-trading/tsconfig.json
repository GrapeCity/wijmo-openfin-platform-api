{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": ".",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "declarationMap": false,
    "target": "es5",
    "module": "commonjs",
    "lib": [ "es2015", "dom" ],
    "declaration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noResolve": false,
    "preserveSymlinks": true,
    "listFiles": true,
    "listEmittedFiles": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": ["openfin"]
  }
}
