{
  "name": "stock-portfolio",
  "version": "2.0.1",
  "description": "A financial sample that demonstrates the usage of Wijmo controls in OpenFin application",
  "scripts": {
    "start": "concurrently -k -s first \"npm:server-chart\" \"npm:server-grid\" \"npm:openfin-grid\"",
    "build": "concurrently \"npm:build-chart\" \"npm:build-grid\"",
    "ng": "ng",
    "start-grid": "concurrently -k -s first \"npm:server-grid\" \"npm:openfin-grid\"",
    "server-grid": "ng serve stock-portfolio --port 4200",
    "openfin-grid": "wait-on http://localhost:4200/ && openfin -l -c http://localhost:4200/app.json",
    "build-grid": "ng build stock-portfolio",
    "test-grid": "ng test stock-portfolio",
    "lint-grid": "ng lint stock-portfolio",
    "e2e-grid": "ng e2e stock-portfolio",
    "start-chart": "concurrently -k -s first \"npm:server-chart\" \"npm:openfin-chart\"",
    "server-chart": "ng serve stock-portfolio-chart --port 4300",
    "openfin-chart": "wait-on http://localhost:4300/ && openfin -l -c http://localhost:4300/app.json",
    "build-chart": "ng build stock-portfolio-chart",
    "test-chart": "ng test stock-portfolio-chart",
    "lint-chart": "ng lint stock-portfolio-chart",
    "e2e-chart": "ng e2e stock-portfolio-chart",
    "update-version": "npm --no-git-tag-version version patch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "@grapecity/wijmo": "~5.20192.624",
    "@grapecity/wijmo.angular2.chart": "~5.20192.624",
    "@grapecity/wijmo.angular2.core": "~5.20192.624",
    "@grapecity/wijmo.angular2.directivebase": "~5.20192.624",
    "@grapecity/wijmo.angular2.grid": "~5.20192.624",
    "@grapecity/wijmo.angular2.input": "~5.20192.624",
    "@grapecity/wijmo.chart": "~5.20192.624",
    "@grapecity/wijmo.grid": "~5.20192.624",
    "@grapecity/wijmo.input": "~5.20192.624",
    "@grapecity/wijmo.styles": "~5.20192.624",
    "bootstrap": "3.4.1",
    "bootswatch": "3.4.1",
    "openfin-layouts": "^1.2.0",
    "openfin-notifications": "^0.11.1",
    "rxjs": "~6.4.0",
    "stock-core": "file:../stock-core",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.0",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/openfin": "^49.0.0",
    "codelyzer": "^5.0.0",
    "concurrently": "4.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "openfin-cli": "2.0.10",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "wait-on": "3.3.0"
  }
}
